{
  "expo": {
    "name": "Nubank Manual",
    "slug": "nubank-manual",
    "scheme": "nubankmanual",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jvs.nubankmanual",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permitir que o app selecione arquivos (futuro)."
      }
    },
    "android": {
      "package": "com.jvs.nubankmanual"
    },
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}